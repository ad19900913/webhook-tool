{
  "server": {
    "port": 3000,
    "host": "localhost"
  },
  "webhook": {
    "maxPayloadSize": "5mb",
    "rateLimit": {
      "max": 200
    },
    "security": {
      "enableIpWhitelist": false,
      "ipWhitelist": ["127.0.0.1", "::1"]
    }
  },
  "logging": {
    "level": "debug",
    "maxSize": "50mb"
  },
  "storage": {
    "maxLogs": 5000
  },
  "ui": {
    "theme": "auto",
    "pageSize": 25
  }
}
